shadow peg_literal_sequence {
    assert (quick_match "\"a\" \"b\"" "ab")
    assert (== (quick_match "\"a\" \"b\"" "ac") false)
}

shadow peg_choice {
    assert (quick_match "\"a\" / \"b\"" "b")
    assert (== (quick_match "\"a\" / \"b\"" "c") false)
}

shadow peg_class_plus_capture {
    let caps: array<string> = (quick_captures "(\"a\" [0-9]+) \"z\"" "a123z")
    assert (== (array_length caps) 1)
    assert (== (at caps 0) "a123")
}
