# Test basic opaque type parsing
#extern fn nl_test_opaque() -> opaque

extern fn nl_test_opaque_param(ptr: opaque) -> int

fn test_param() -> int {
    return 42
}

shadow test_param {
    assert (== (test_param) 42)
}

fn main() -> int {
    let result: int = (test_param)
    assert (== result 42)
    return 0
}

shadow main {
    assert (== (main) 0)
}

