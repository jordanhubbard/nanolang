from "std/regex/regex.nano" import regex_split

fn main() -> int {
    let text: string = "a,b;c"
    let parts: array<string> = (regex_split "[,;]" text)
    
    (println (+ "Split " (+ text (+ " into " (+ (int_to_string (array_length parts)) " parts")))))
    
    let mut i: int = 0
    while (< i (array_length parts)) {
        let part: string = (array_get parts i)
        (println (+ "  [" (+ (int_to_string i) (+ "] = '" (+ part "'")))))
        set i (+ i 1)
    }
    
    return 0
}

