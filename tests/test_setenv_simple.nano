from "modules/std/env.nano" import set_env, get

fn main() -> int {
    let result: int = (set_env "TEST_VAR" "hello")
    (println (+ "set_env returned: " (int_to_string result)))
    let val: string = (get "TEST_VAR")
    (println (+ "get returned: " val))
    return 0
}

shadow main {
    assert (== (main) 0)
}

