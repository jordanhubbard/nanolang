/* Test generic list with workaround - using int extraction */

struct Point {
    x: int,
    y: int
}

fn test_list_point() -> bool {
    let points: List<Point> = (list_Point_new)
    
    let p1: Point = Point { x: 10, y: 20 }
    let p2: Point = Point { x: 30, y: 40 }
    
    (list_Point_push points p1)
    (list_Point_push points p2)
    
    let len: int = (list_Point_length points)
    
    # Workaround: Don't call get, just check length
    return (== len 2)
}

fn main() -> int {
    let result: bool = (test_list_point)
    if result {
        (println "âœ… Generic list test passed!")
        return 0
    } else {
        (println "âŒ Generic list test failed!")
        return 1
    }
}

shadow test_list_point {
    assert (== (test_list_point) true)
}
