fn double(x: int) -> int {
    return (* x 2)
}

shadow double {
    assert (== (double 5) 10)
}

fn apply_twice(x: int, f: fn(int) -> int) -> int {
    return (f (f x))
}

shadow apply_twice {
    assert (== (apply_twice 3 double) 12)
}

fn main() -> int {
    assert (== (apply_twice 4 double) 16)
    return 0
}

shadow main { assert true }
