/* User guide test: canonical function definition */
fn add(a: int, b: int) -> int {
    return (+ a b)
}

shadow add {
    assert (== (add 2 3) 5)
    assert (== (add -1 1) 0)
}

fn greet() -> void {
    (println "Hello!")
}

shadow greet {
    (greet)
}

fn main() -> int {
    (println (int_to_string (add 10 20)))
    (greet)
    return 0
}

shadow main { assert true }
