fn sum_range(n: int) -> int {
    let mut total: int = 0
    for i in (range 0 n) {
        set total (+ total i)
    }
    return total
}

shadow sum_range {
    assert (== (sum_range 5) 10)  # 0+1+2+3+4
    assert (== (sum_range 0) 0)
}

fn main() -> int {
    assert (== (sum_range 10) 45)
    return 0
}

shadow main { assert true }
