fn classify(n: int) -> string {
    return (cond
        ((< n 0) "negative")
        ((== n 0) "zero")
        (else "positive")
    )
}

shadow classify {
    assert (== (classify -5) "negative")
    assert (== (classify 0) "zero")
    assert (== (classify 42) "positive")
}

fn main() -> int {
    (println (classify 10))
    return 0
}

shadow main { assert true }
