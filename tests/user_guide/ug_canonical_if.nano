fn print_sign(n: int) -> void {
    if (< n 0) {
        (println "negative")
    } else {
        if (== n 0) {
            (println "zero")
        } else {
            (println "positive")
        }
    }
}

shadow print_sign {
    (print_sign -1)
    (print_sign 0)
    (print_sign 1)
}

fn main() -> int {
    (print_sign 42)
    return 0
}

shadow main { assert true }
