struct Point {
    x: int
    y: int
}

fn make_point(x: int, y: int) -> Point {
    return Point { x: x, y: y }
}

shadow make_point {
    let p: Point = (make_point 3 4)
    assert (== p.x 3)
    assert (== p.y 4)
}

fn main() -> int {
    let origin: Point = Point { x: 0, y: 0 }
    (println (int_to_string origin.x))
    return 0
}

