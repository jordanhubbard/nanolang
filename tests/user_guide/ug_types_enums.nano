enum Status {
    IDLE = 0,
    RUNNING = 1,
    DONE = 2
}

fn is_running(s: Status) -> bool {
    return (== s Status.RUNNING)
}

shadow is_running {
    assert (is_running Status.RUNNING)
    assert (not (is_running Status.IDLE))
}

fn main() -> int {
    assert (is_running Status.RUNNING)
    return 0
}

shadow main { assert true }
