/* Test: function preconditions with requires clauses */
fn safe_divide(x: int, y: int) -> int
    requires (> x 0)
    requires (!= y 0)
{
    return (/ x y)
}

shadow safe_divide {
    assert (== (safe_divide 12 3) 4)
    assert (== (safe_divide 100 4) 25)
}

fn main() -> int {
    let result: int = (safe_divide 100 4)
    assert (== result 25)
    return 0
}

shadow main {
    assert (== (main) 0)
}
