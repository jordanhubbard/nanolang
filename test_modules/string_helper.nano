/* Simple string helper module for testing import aliases */

/* This module also has functions with same names as math_helper */
/* to test that aliases prevent naming conflicts */

module strings

pub fn cube(s: string) -> string {
    return (str_concat s (str_concat s s))
}

pub fn test_strings() -> int {
    let result: string = (cube "Hi")
    if (str_equals result "HiHiHi") {
        return 1
    } else {
        return 0
    }
}

shadow test_strings {
    assert (== (test_strings) 1)
}
