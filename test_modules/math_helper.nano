/* Simple test module for import alias testing */

module math

pub fn add(a: int, b: int) -> int {
    return (+ a b)
}

pub fn multiply(a: int, b: int) -> int {
    return (* a b)
}

pub fn square(x: int) -> int {
    return (* x x)
}

shadow add {
    assert (== (add 2 3) 5)
}

shadow multiply {
    assert (== (multiply 4 5) 20)
}

shadow square {
    assert (== (square 7) 49)
}
