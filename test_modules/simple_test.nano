/* Simpler test without conflicts */

import "test_modules/math_helper.nano" as Math

fn main() -> int {
    (println "Testing import aliases...")
    let result: int = (Math.square 5)
    if (== result 25) {
        (println "✓ Math.square works!")
        return 0
    } else {
        (println "✗ Failed")
        return 1
    }
}

shadow main {
    assert (== (main) 0)
}
