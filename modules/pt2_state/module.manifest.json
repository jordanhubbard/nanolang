{
  "name": "pt2_state",
  "version": "1.0.0",
  "stability": "experimental",
  "summary": "ProTracker 2 state management for tracker UI.",
  "use_when": [
    "User is building a ProTracker clone",
    "User says 'tracker state', 'pattern editor', 'tracker ui'",
    "User needs tracker playback state"
  ],
  "avoid_when": ["User only wants playback (use pt2_audio)"],
  "capabilities": ["tracker_state", "pattern_state", "playback_position"],
  "keywords": ["protracker", "state", "tracker", "ui"],
  "examples": ["examples/sdl_tracker_shell.nano"],
  "dependencies": {
    "nanolang": ["core>=1.0"],
    "modules": ["pt2_module"],
    "system": []
  },
  "io_surfaces": [],
  "alternatives": []
}
