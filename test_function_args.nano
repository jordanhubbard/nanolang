# Test function call arguments
fn add(a: int, b: int) -> int {
    return (+ a b)
}

shadow add {
    assert (== (add 5 3) 8)
}

fn greet(name: string) -> string {
    return (str_concat "Hello, " name)
}

shadow greet {
    assert (== (greet "Alice") "Hello, Alice")
}

fn main() -> int {
    (println "Function call arguments test")
    return 0
}

shadow main {
    assert (== (main) 0)
}

