# Array Test - Comprehensive shadow tests

fn test_literal() -> int {
    return (array_length [1, 2, 3])
}

shadow test_literal {
    # Test that array literal works
    assert (== (test_literal) 3)
}

fn test_at() -> int {
    return (at [10, 20, 30] 1)
}

shadow test_at {
    # Test array access
    assert (== (test_at) 20)
}

fn test_sum() -> int {
    return (+ (+ (at [5, 10, 15] 0) (at [5, 10, 15] 1)) (at [5, 10, 15] 2))
}

shadow test_sum {
    # Test multiple accesses
    assert (== (test_sum) 30)
}

fn main() -> int {
    return 0
}

shadow main {
    assert (== (main) 0)
}

