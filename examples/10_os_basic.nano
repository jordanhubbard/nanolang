# Basic OS functions test

fn test_os() -> int {
    # Test getcwd
    let cwd: string = (getcwd)
    print "Current directory: "
    print cwd

    # Test getenv
    let home: string = (getenv "HOME")
    print "Home directory: "
    print home

    return 0
}

shadow test_os {
    assert (== (test_os) 0)
}

fn main() -> int {
    return (test_os)
}

shadow main {
    assert (== (main) 0)
}
