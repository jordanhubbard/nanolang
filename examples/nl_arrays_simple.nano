# Simple array test - just verify basic operations work

fn main() -> int {
    (println "Testing arrays...")
    
    # Create an array literal
    (println [1, 2, 3, 4, 5])
    
    # Create array with array_new
    (println (array_new 3 42))
    
    # Test array_length
    (println (array_length [10, 20, 30]))
    
    # Test at()
    (println (at [5, 6, 7] 0))
    (println (at [5, 6, 7] 2))
    
    (println "Arrays work!")
    return 0
}

shadow main {
    assert (== (main) 0)
}

