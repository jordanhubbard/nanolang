{
  "Function": {
    "prefix": "fn",
    "body": [
      "fn ${1:function_name}(${2:params}) -> ${3:int} {",
      "\t${4:return 0}",
      "}",
      "",
      "shadow ${1:function_name} {",
      "\tassert ${5:condition}",
      "}"
    ],
    "description": "Create a function with shadow test"
  },
  "Main Function": {
    "prefix": "main",
    "body": [
      "fn main() -> int {",
      "\t${1:(println \"Hello, NanoLang!\")}",
      "\treturn 0",
      "}",
      "",
      "shadow main {",
      "\tassert (== (main) 0)",
      "}"
    ],
    "description": "Create main function"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:StructName} {",
      "\t${2:field}: ${3:int}",
      "}"
    ],
    "description": "Create a struct"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:EnumName} {",
      "\t${2:Variant1}",
      "\t${3:Variant2}",
      "}"
    ],
    "description": "Create an enum"
  },
  "Union": {
    "prefix": "union",
    "body": [
      "union ${1:UnionName}<${2:T}> {",
      "\t${3:Some}(${2:T})",
      "\t${4:None}",
      "}"
    ],
    "description": "Create a union (tagged union/sum type)"
  },
  "Match": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "\t${2:Pattern1} => ${3:result1}",
      "\t${4:Pattern2} => ${5:result2}",
      "\t_ => ${6:default}",
      "}"
    ],
    "description": "Create a match expression"
  },
  "If-Else": {
    "prefix": "if",
    "body": [
      "if ${1:condition} {",
      "\t${2:// then}",
      "} else {",
      "\t${3:// else}",
      "}"
    ],
    "description": "Create an if-else statement"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "\t${2:// body}",
      "}"
    ],
    "description": "Create a while loop"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} in (range ${2:0} ${3:10}) {",
      "\t${4:// body}",
      "}"
    ],
    "description": "Create a for loop with range"
  },
  "Shadow Test": {
    "prefix": "shadow",
    "body": [
      "shadow ${1:function_name} {",
      "\tassert ${2:condition}",
      "}"
    ],
    "description": "Create a shadow test block"
  },
  "Let Variable": {
    "prefix": "let",
    "body": "let ${1:name}: ${2:int} = ${3:value}",
    "description": "Declare an immutable variable"
  },
  "Let Mutable": {
    "prefix": "letmut",
    "body": "let mut ${1:name}: ${2:int} = ${3:value}",
    "description": "Declare a mutable variable"
  },
  "Import": {
    "prefix": "import",
    "body": "from \"${1:module/path}\" import ${2:function}",
    "description": "Import from module"
  },
  "Extern Function": {
    "prefix": "extern",
    "body": "extern fn ${1:function_name}(${2:params}) -> ${3:int}",
    "description": "Declare external C function"
  },
  "Array Declaration": {
    "prefix": "array",
    "body": "let ${1:name}: array<${2:int}> = [${3:1, 2, 3}]",
    "description": "Declare an array"
  },
  "Result Ok": {
    "prefix": "ok",
    "body": "Result::Ok(${1:value})",
    "description": "Return Result Ok variant"
  },
  "Result Err": {
    "prefix": "err",
    "body": "Result::Err(${1:error})",
    "description": "Return Result Err variant"
  },
  "Option Some": {
    "prefix": "some",
    "body": "Option::Some(${1:value})",
    "description": "Return Option Some variant"
  },
  "Option None": {
    "prefix": "none",
    "body": "Option::None",
    "description": "Return Option None variant"
  },
  "Prefix Add": {
    "prefix": "+",
    "body": "(+ ${1:a} ${2:b})",
    "description": "Addition in prefix notation"
  },
  "Prefix Subtract": {
    "prefix": "-",
    "body": "(- ${1:a} ${2:b})",
    "description": "Subtraction in prefix notation"
  },
  "Prefix Multiply": {
    "prefix": "*",
    "body": "(* ${1:a} ${2:b})",
    "description": "Multiplication in prefix notation"
  },
  "Prefix Divide": {
    "prefix": "/",
    "body": "(/ ${1:a} ${2:b})",
    "description": "Division in prefix notation"
  },
  "Equals": {
    "prefix": "==",
    "body": "(== ${1:a} ${2:b})",
    "description": "Equality comparison"
  },
  "Print": {
    "prefix": "print",
    "body": "(println ${1:message})",
    "description": "Print line"
  }
}
