    set sb (sb_append sb "static DynArray* nl_filter(DynArray* arr, int (*fn)(int64_t)) {\n")
    set sb (sb_append sb "    DynArray* result = dyn_array_new(ELEM_INT);\n")
    set sb (sb_append sb "    int64_t len = dyn_array_length(arr);\n")
    set sb (sb_append sb "    for (int64_t i = 0; i < len; i++) {\n")
    set sb (sb_append sb "        int64_t val = dyn_array_get_int(arr, i);\n")
    set sb (sb_append sb "        if (fn(val)) dyn_array_push_int(result, val);\n")
    set sb (sb_append sb "    }\n")
    set sb (sb_append sb "    return result;\n")
    set sb (sb_append sb "}\n\n")

    /* HashMap wrappers (generic API over type-specific implementations) */
    set sb (sb_append sb "/* HashMap wrappers */\n")
    set sb (sb_append sb "static inline HashMap_string_int* nl_map_new(void) { return nl_hashmap_string_int_alloc(16); }\n")
    set sb (sb_append sb "static inline void nl_map_put(HashMap_string_int* hm, char* key, int64_t val) { nl_hashmap_string_int_put(hm, key, val); }\n")
    set sb (sb_append sb "static inline int64_t nl_map_get(HashMap_string_int* hm, char* key) { return nl_hashmap_string_int_get(hm, key); }\n")
    set sb (sb_append sb "static inline int nl_map_has(HashMap_string_int* hm, char* key) { return nl_hashmap_string_int_has(hm, key); }\n")
    set sb (sb_append sb "static inline int64_t nl_map_size(HashMap_string_int* hm) { return hm->size; }\n\n")

    /* Timing utilities */
    set sb (sb_append sb "/* Timing utilities */\n")
    set sb (sb_append sb "static int64_t nl_timing_get_microseconds(void) {\n")
    set sb (sb_append sb "#ifdef CLOCK_REALTIME\n")
    set sb (sb_append sb "    struct timespec ts;\n")
    set sb (sb_append sb "    clock_gettime(CLOCK_REALTIME, &ts);\n")
    set sb (sb_append sb "    return ((int64_t)ts.tv_sec * 1000000LL) + (int64_t)(ts.tv_nsec / 1000);\n")
    set sb (sb_append sb "#else\n")
    set sb (sb_append sb "    struct timeval tv;\n")
    set sb (sb_append sb "    gettimeofday(&tv, NULL);\n")
    set sb (sb_append sb "    return ((int64_t)tv.tv_sec * 1000000LL) + (int64_t)tv.tv_usec;\n")
    set sb (sb_append sb "#endif\n")
    set sb (sb_append sb "}\n\n")

    set sb (sb_append sb "static int64_t nl_timing_get_nanoseconds(void) {\n")
    set sb (sb_append sb "#ifdef __MACH__\n")
    set sb (sb_append sb "    static mach_timebase_info_data_t timebase;\n")
    set sb (sb_append sb "    static int initialized = 0;\n")
    set sb (sb_append sb "    if (!initialized) {\n")
    set sb (sb_append sb "        mach_timebase_info(&timebase);\n")
    set sb (sb_append sb "        initialized = 1;\n")
    set sb (sb_append sb "    }\n")
    set sb (sb_append sb "    uint64_t mach_time = mach_absolute_time();\n")
    set sb (sb_append sb "    return (int64_t)((mach_time * timebase.numer) / timebase.denom);\n")
    set sb (sb_append sb "#elif defined(CLOCK_MONOTONIC)\n")
    set sb (sb_append sb "    struct timespec ts;\n")
    set sb (sb_append sb "    clock_gettime(CLOCK_MONOTONIC, &ts);\n")
    set sb (sb_append sb "    return ((int64_t)ts.tv_sec * 1000000000LL) + (int64_t)ts.tv_nsec;\n")
    set sb (sb_append sb "#else\n")
    set sb (sb_append sb "    return nl_timing_get_microseconds() * 1000LL;\n")
    set sb (sb_append sb "#endif\n")
    set sb (sb_append sb "}\n\n")

    set sb (sb_append sb "/* ========== End Math and Utility Built-in Functions ========== */\n\n")

    return (sb_build sb)
}

