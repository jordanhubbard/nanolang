/* Emit C typedefs for one union instantiation (or non-generic union) */
fn emit_union_typedefs(u: ASTUnion, mono_name: string, generic_params: array<string>, type_args: array<string>) -> string {
    let mut sb: array<string> = []
    let c_prefix: string = (+ "nl_" mono_name)

    /* Emit variant structs */
    let mut vi: int = 0
    let mut field_offset: int = 0
    while (< vi u.variant_count) {
        let vname: string = (at u.variant_names vi)
        let fcount: int = (at u.variant_field_counts vi)

        set sb (sb_append sb "typedef struct { ")
        if (== fcount 0) {
            set sb (sb_append sb "int _placeholder; ")
        } else {
            let mut fi: int = 0
            while (< fi fcount) {
                let fname: string = (at u.variant_field_names (+ field_offset fi))
                let ftype: string = (at u.variant_field_types (+ field_offset fi))
                let c_ftype: string = (substitute_union_field_type ftype generic_params type_args)
                set sb (sb_append sb c_ftype)
                set sb (sb_append sb " ")
                set sb (sb_append sb (mangle_name fname))
                set sb (sb_append sb "; ")
                set fi (+ fi 1)
            }
        }
        set sb (sb_append sb "} ")
        set sb (sb_append sb c_prefix)
        set sb (sb_append sb "_")
        set sb (sb_append sb vname)
        set sb (sb_append sb ";\n")

        set field_offset (+ field_offset fcount)
        set vi (+ vi 1)
    }

    /* Emit tag enum */
    set sb (sb_append sb "typedef enum { ")
    set vi 0
    while (< vi u.variant_count) {
        if (> vi 0) {
            set sb (sb_append sb ", ")
        } else { (print "") }
        let vname2: string = (at u.variant_names vi)
        set sb (sb_append sb c_prefix)
        set sb (sb_append sb "_TAG_")
        set sb (sb_append sb vname2)
        set sb (sb_append sb " = ")
        set sb (sb_append sb (int_to_string vi))
        set vi (+ vi 1)
    }
    set sb (sb_append sb " } ")
    set sb (sb_append sb c_prefix)
    set sb (sb_append sb "_Tag;\n")

    /* Emit main union struct */
    set sb (sb_append sb "typedef struct ")
    set sb (sb_append sb c_prefix)
    set sb (sb_append sb " { ")
    set sb (sb_append sb c_prefix)
    set sb (sb_append sb "_Tag tag; union { ")
    set vi 0
    while (< vi u.variant_count) {
        let vname3: string = (at u.variant_names vi)
        set sb (sb_append sb c_prefix)
        set sb (sb_append sb "_")
        set sb (sb_append sb vname3)
        set sb (sb_append sb " ")
        set sb (sb_append sb vname3)
        set sb (sb_append sb "; ")
        set vi (+ vi 1)
    }
    set sb (sb_append sb "} data; } ")
    set sb (sb_append sb c_prefix)
    set sb (sb_append sb ";\n")

    return (sb_build sb)
}

