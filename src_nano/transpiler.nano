}

fn mangle_type_name(name: string) -> string {
    let mut out: string = ""
    let len: int = (str_length name)
    let mut i: int = 0

    while (< i len) {
        let ch: string = (str_substring name i 1)

        if (and (== ch ":") (< (+ i 1) len)) {
            let ch2: string = (str_substring name (+ i 1) 1)
            if (== ch2 ":") {
                set out (+ out "_")
                set i (+ i 2)
            } else {
                set out (+ out "_")
                set i (+ i 1)
            }
        } else {
            if (or (== ch ".")
                    (or (== ch "<")
                        (or (== ch ">")
                            (or (== ch ",")
                                (or (== ch "(")
                                    (or (== ch ")")
                                        (== ch " "))))))) {
                set out (+ out "_")
            } else {
                set out (+ out ch)
            }
            set i (+ i 1)
        }
    }

    return out
}

