/* Generate List<T> specializations for all detected types */
fn generate_list_specializations(parser: Parser) -> string {
    let list_types: array<string> = (detect_list_types parser)
    let type_count: int = (array_length list_types)
    
    if (<= type_count 0) {
        return ""
    } else {
        let mut result: string = "\n/* ========== Generic List Specializations ========== */\n\n"
        
        let mut i: int = 0
        while (< i type_count) {
            let elem_type: string = (at list_types i)
            
            /* Skip built-in types that already have C runtime implementations */
            if (or (== elem_type "int") (or (== elem_type "string") (== elem_type "LexerToken"))) {
                (print "")
            } else {
                set result (+ result (generate_list_for_type elem_type))
                set result (+ result "\n")
            }
            
            set i (+ i 1)
        }
        
        set result (+ result "/* ========== End Generic List Specializations ========== */\n\n")
        return result
    }
}

/* Generate List<T> struct and functions for a specific type T */
fn generate_list_for_type(elem_type: string) -> string {
    let c_elem_type: string = (type_to_c elem_type)
    let list_name: string = (+ "List_" elem_type)
    
    let mut code: string = ""
    
    /* Struct definition */
    set code (+ code "typedef struct ")
    set code (+ code list_name)
    set code (+ code " {\n")
    set code (+ code "    ")
    set code (+ code c_elem_type)
    set code (+ code " *data;\n")
    set code (+ code "    int count;\n")
    set code (+ code "    int capacity;\n")
    set code (+ code "} ")
    set code (+ code list_name)
    set code (+ code ";\n\n")
    
    /* Constructor: list_T_new() */
    set code (+ code list_name)
    set code (+ code "* list_")
    set code (+ code elem_type)
    set code (+ code "_new() {\n")
    set code (+ code "    ")
    set code (+ code list_name)
    set code (+ code " *list = malloc(sizeof(")
    set code (+ code list_name)
    set code (+ code "));\n")
    set code (+ code "    if (!list) return NULL;\n")
    set code (+ code "    list->data = malloc(sizeof(")
    set code (+ code c_elem_type)
    set code (+ code ") * 4);\n")
    set code (+ code "    if (!list->data) { free(list); return NULL; }\n")
    set code (+ code "    list->count = 0;\n")
    set code (+ code "    list->capacity = 4;\n")
    set code (+ code "    return list;\n")
    set code (+ code "}\n\n")
    
    /* Push: list_T_push(list, value) */
    set code (+ code "void list_")
    set code (+ code elem_type)
    set code (+ code "_push(")
    set code (+ code list_name)
    set code (+ code " *list, ")
    set code (+ code c_elem_type)
    set code (+ code " value) {\n")
    set code (+ code "    if (list->count >= list->capacity) {\n")
    set code (+ code "        list->capacity *= 2;\n")
    set code (+ code "        ")
    set code (+ code c_elem_type)
    set code (+ code " *new_data = realloc(list->data, sizeof(")
    set code (+ code c_elem_type)
    set code (+ code ") * list->capacity);\n")
    set code (+ code "        if (!new_data) return;\n")
    set code (+ code "        list->data = new_data;\n")
    set code (+ code "    }\n")
    set code (+ code "    list->data[list->count++] = value;\n")
    set code (+ code "}\n\n")
    
    /* Get: list_T_get(list, index) */
    set code (+ code c_elem_type)
    set code (+ code " list_")
    set code (+ code elem_type)
    set code (+ code "_get(")
    set code (+ code list_name)
    set code (+ code " *list, int index) {\n")
    set code (+ code "    return list->data[index];\n")
    set code (+ code "}\n\n")
    
    /* Length: list_T_length(list) */
    set code (+ code "int list_")
    set code (+ code elem_type)
    set code (+ code "_length(")
    set code (+ code list_name)
    set code (+ code " *list) {\n")
    set code (+ code "    return list->count;\n")
    set code (+ code "}\n")
    
    return code
}

