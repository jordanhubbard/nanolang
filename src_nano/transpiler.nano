/* Generate runtime helper functions */
fn gen_c_runtime() -> string {
    let mut result: string = "/* Runtime helper functions */\n"
    
    /* print function */
    set result (+ result "void nl_print(char* s) {\n")
    set result (+ result "    printf(\"%s\", s);\n")
    set result (+ result "}\n\n")
    
    /* println function */
    set result (+ result "void nl_println(char* s) {\n")
    set result (+ result "    printf(\"%s\\n\", s);\n")
    set result (+ result "}\n\n")
    
    /* int_to_string function */
    set result (+ result "char* nl_int_to_string(int64_t n) {\n")
    set result (+ result "    char* buf = malloc(32);\n")
    set result (+ result "    snprintf(buf, 32, \"%lld\", n);\n")
    set result (+ result "    return buf;\n")
    set result (+ result "}\n\n")

    /* string concat helper */
    set result (+ result "char* nl_str_concat(const char* a, const char* b) {\n")
    set result (+ result "    size_t la = strlen(a);\n")
    set result (+ result "    size_t lb = strlen(b);\n")
    set result (+ result "    char* out = (char*)malloc(la + lb + 1);\n")
    set result (+ result "    memcpy(out, a, la);\n")
    set result (+ result "    memcpy(out + la, b, lb);\n")
    set result (+ result "    out[la + lb] = 0;\n")
    set result (+ result "    return out;\n")
    set result (+ result "}\n\n")
    
    /* string length function */
    set result (+ result "int64_t nl_str_length(const char* s) {\n")
    set result (+ result "    return (int64_t)strlen(s);\n")
    set result (+ result "}\n\n")

    /* char_at: return byte at index (ASCII-safe) */
    set result (+ result "int64_t nl_char_at(const char* s, int64_t i) {\n")
    set result (+ result "    return (int64_t)((unsigned char)s[i]);\n")
    set result (+ result "}\n\n")

    /* substring: allocate a new null-terminated slice */
    set result (+ result "char* nl_str_substring(const char* s, int64_t start, int64_t len) {\n")
    set result (+ result "    if (!s) return NULL;\n")
    set result (+ result "    int64_t n = (int64_t)strlen(s);\n")
    set result (+ result "    if (start < 0) start = 0;\n")
    set result (+ result "    if (len < 0) len = 0;\n")
    set result (+ result "    if (start > n) start = n;\n")
    set result (+ result "    if (start + len > n) len = n - start;\n")
    set result (+ result "    char* out = (char*)malloc((size_t)len + 1);\n")
    set result (+ result "    if (!out) return NULL;\n")
    set result (+ result "    memcpy(out, s + start, (size_t)len);\n")
    set result (+ result "    out[len] = 0;\n")
    set result (+ result "    return out;\n")
    set result (+ result "}\n\n")

    /* Module std_fs FFI (unprefixed symbols when sources are merged directly) */
    set result (+ result "/* std_fs externs (provided by modules/std/fs.c) */\n")
    set result (+ result "extern DynArray* fs_walkdir(const char* root);\n")
    set result (+ result "extern const char* path_normalize(const char* path);\n")
    set result (+ result "extern const char* path_join(const char* a, const char* b);\n")
    set result (+ result "extern const char* path_basename(const char* path);\n")
    set result (+ result "extern const char* path_dirname(const char* path);\n")
    set result (+ result "extern char* file_read(const char* path);\n")
    set result (+ result "extern int64_t file_write(const char* path, const char* content);\n")
    set result (+ result "extern int64_t file_append(const char* path, const char* content);\n")
    set result (+ result "extern bool file_exists(const char* path);\n")
    set result (+ result "extern int64_t file_delete(const char* path);\n\n")
    
    /* min function */
    set result (+ result "#define nl_min(a, b) ((a) < (b) ? (a) : (b))\n\n")
    
    /* max function */
    set result (+ result "#define nl_max(a, b) ((a) > (b) ? (a) : (b))\n\n")
    
    return result
}

