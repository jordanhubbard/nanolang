}

fn last_slash_index(s: string) -> int {
    let len: int = (str_length s)
    let mut i: int = 0
    let mut last: int = -1
    while (< i len) {
        if (== (str_substring s i 1) "/") {
            set last i
        } else { (print "") }
        set i (+ i 1)
    }
    return last
}

