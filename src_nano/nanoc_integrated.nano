/* Get current token from token stream */
fn parser_current(p: Parser) -> Token {
    if (parser_is_at_end p) {
        /* Return EOF token */
        return Token {
            token_type: 0,  /* EOF */
            value: "",
            line: 0,
            column: 0
        }
    } else {
        return (list_Token_get p.tokens p.position)
    }
}

