    assert (> result 0)
    assert (> (list_token_length tokens) 0)
}

# Process single character token - returns new position
fn process_single_char(c: int, tokens: List<Token>, line: int, column: int, i: int) -> int {
    if (== c 40) {
        let tok: Token = Token { token_type: TokenType.TOKEN_LPAREN, value: "", line: line, column: column }
        (List_Token_push tokens tok)
        return (+ i 1)
    } else {
        if (== c 41) {
            let tok: Token = Token { token_type: TokenType.TOKEN_RPAREN, value: "", line: line, column: column }
            (List_Token_push tokens tok)
            return (+ i 1)
        } else {
            if (== c 123) {
                let tok: Token = Token { token_type: TokenType.TOKEN_LBRACE, value: "", line: line, column: column }
                (List_Token_push tokens tok)
                return (+ i 1)
            } else {
                if (== c 125) {
                    let tok: Token = Token { token_type: TokenType.TOKEN_RBRACE, value: "", line: line, column: column }
                    (List_Token_push tokens tok)
                    return (+ i 1)
                } else {
                    if (== c 91) {
                        let tok: Token = Token { token_type: TokenType.TOKEN_LBRACKET, value: "", line: line, column: column }
                        (List_Token_push tokens tok)
                        return (+ i 1)
                    } else {
                        if (== c 93) {
                            let tok: Token = Token { token_type: TokenType.TOKEN_RBRACKET, value: "", line: line, column: column }
                            (List_Token_push tokens tok)
                            return (+ i 1)
                        } else {
                            if (== c 44) {
                                let tok: Token = Token { token_type: TokenType.TOKEN_COMMA, value: "", line: line, column: column }
                                (List_Token_push tokens tok)
                                return (+ i 1)
                            } else {
                                if (== c 58) {
                                    let tok: Token = Token { token_type: TokenType.TOKEN_COLON, value: "", line: line, column: column }
                                    (List_Token_push tokens tok)
                                    return (+ i 1)
                                } else {
                                    if (== c 46) {
                                        let tok: Token = Token { token_type: TokenType.TOKEN_DOT, value: "", line: line, column: column }
                                        (List_Token_push tokens tok)
                                        return (+ i 1)
                                    } else {
                                        if (== c 43) {
                                            let tok: Token = Token { token_type: TokenType.TOKEN_PLUS, value: "", line: line, column: column }
                                            (List_Token_push tokens tok)
                                            return (+ i 1)
                                        } else {
                                            if (== c 45) {
                                                let tok: Token = Token { token_type: TokenType.TOKEN_MINUS, value: "", line: line, column: column }
                                                (List_Token_push tokens tok)
                                                return (+ i 1)
                                            } else {
                                                if (== c 42) {
                                                    let tok: Token = Token { token_type: TokenType.TOKEN_STAR, value: "", line: line, column: column }
                                                    (List_Token_push tokens tok)
                                                    return (+ i 1)
                                                } else {
                                                    if (== c 47) {
                                                        let tok: Token = Token { token_type: TokenType.TOKEN_SLASH, value: "", line: line, column: column }
                                                        (List_Token_push tokens tok)
                                                        return (+ i 1)
                                                    } else {
                                                        if (== c 37) {
                                                            let tok: Token = Token { token_type: TokenType.TOKEN_PERCENT, value: "", line: line, column: column }
                                                            (List_Token_push tokens tok)
                                                            return (+ i 1)
                                                        } else {
                                                            if (== c 60) {
                                                                let tok: Token = Token { token_type: TokenType.TOKEN_LT, value: "", line: line, column: column }
                                                                (List_Token_push tokens tok)
                                                                return (+ i 1)
                                                            } else {
                                                                if (== c 62) {
                                                                    let tok: Token = Token { token_type: TokenType.TOKEN_GT, value: "", line: line, column: column }
                                                                    (List_Token_push tokens tok)
                                                                    return (+ i 1)
                                                                } else {
                                                                    return (+ i 1)
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
