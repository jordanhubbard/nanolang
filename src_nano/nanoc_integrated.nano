/* Check if two types are equal */
fn types_equal(t1: Type, t2: Type) -> bool {
    if (!= t1.kind t2.kind) {
        return false
    } else {
        /* For struct types, also check name */
        if (== t1.kind TypeKind.TYPE_STRUCT) {
            return (== t1.name t2.name)
        } else {
            return true
        }
    }
}

