/* Helper: convert nanolang type to C type */
fn type_to_c(nano_type: string) -> string {
    if (== nano_type "int") {
        return "int64_t"
    } else {
        if (== nano_type "float") {
            return "double"
        } else {
            if (== nano_type "bool") {
                return "int"
            } else {
                if (== nano_type "string") {
                    return "char*"
                } else {
                    if (== nano_type "void") {
                        return "void"
                    } else {
                        return nano_type  /* Keep struct types as-is */
                    }
                }
            }
        }
    }
}

