}

/* Create type from string representation */
fn type_from_string(s: string) -> Type {
    if (== s "int") {
        return (type_int)
    } else {
        if (== s "float") {
            return (type_float)
        } else {
            if (== s "bool") {
                return (type_bool)
            } else {
                if (== s "string") {
                    return (type_string)
                } else {
                    if (== s "void") {
                        return (type_void)
                    } else {
                        /* Assume it's a struct type */
                        return Type {
                            kind: TypeKind.TYPE_STRUCT,
                            name: s
                        }
                    }
                }
            }
        }
    }
}

