/* Check if current token matches a specific type */
fn parser_match(p: Parser, token_type: int) -> bool {
    if (parser_is_at_end p) {
        return false
    } else {
        let tok: Token = (parser_current p)
        return (== tok.token_type token_type)
    }
}

