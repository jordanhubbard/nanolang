/* Advance parser to next token - returns new parser with advanced position */
fn parser_advance(p: Parser) -> Parser {
    if (not (parser_is_at_end p)) {
        return (parser_with_position p (+ p.position 1))
    } else {
        return p  /* Already at end */
    }
}

